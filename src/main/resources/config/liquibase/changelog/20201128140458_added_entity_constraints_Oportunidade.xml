<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Oportunidade.
    -->
    <changeSet id="20201128140458-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="oportunidade_perdida_id"
                                 baseTableName="oportunidade"
                                 constraintName="fk_oportunidade_oportunidade_perdida_id"
                                 referencedColumnNames="id"
                                 referencedTableName="oportunidade_perdida"/>

        <addForeignKeyConstraint baseColumnNames="atendimentos_oportunidades_id"
                                 baseTableName="oportunidade"
                                 constraintName="fk_oportunidade_atendimentos_oportunidades_id"
                                 referencedColumnNames="id"
                                 referencedTableName="atendimento"/>

    </changeSet>
</databaseChangeLog>
