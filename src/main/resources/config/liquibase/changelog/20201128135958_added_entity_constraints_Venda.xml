<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Venda.
    -->
    <changeSet id="20201128135958-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="consultor_venda_id"
                                 baseTableName="venda"
                                 constraintName="fk_venda_consultor_venda_id"
                                 referencedColumnNames="id"
                                 referencedTableName="consultor"/>

        <addForeignKeyConstraint baseColumnNames="oportunidade_venda_id"
                                 baseTableName="venda"
                                 constraintName="fk_venda_oportunidade_venda_id"
                                 referencedColumnNames="id"
                                 referencedTableName="oportunidade"/>

    </changeSet>
</databaseChangeLog>
