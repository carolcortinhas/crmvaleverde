<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Cliente.
    -->
    <changeSet id="20201128134958-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="cliente_pessoa_juridica_id"
                                 baseTableName="cliente"
                                 constraintName="fk_cliente_cliente_pessoa_juridica_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cliente_pessoa_juridica"/>

        <addForeignKeyConstraint baseColumnNames="cliente_pessoa_fisica_id"
                                 baseTableName="cliente"
                                 constraintName="fk_cliente_cliente_pessoa_fisica_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cliente_pessoa_fisica"/>

        <addForeignKeyConstraint baseColumnNames="consultor_id"
                                 baseTableName="cliente"
                                 constraintName="fk_cliente_consultor_id"
                                 referencedColumnNames="id"
                                 referencedTableName="consultor"/>

    </changeSet>
</databaseChangeLog>
