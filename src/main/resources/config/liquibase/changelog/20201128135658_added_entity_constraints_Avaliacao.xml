<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Avaliacao.
    -->
    <changeSet id="20201128135658-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="avaliacao_produto_id"
                                 baseTableName="avaliacao"
                                 constraintName="fk_avaliacao_avaliacao_produto_id"
                                 referencedColumnNames="id"
                                 referencedTableName="avaliacao_produto"/>

        <addForeignKeyConstraint baseColumnNames="avaliacao_atendimento_id"
                                 baseTableName="avaliacao"
                                 constraintName="fk_avaliacao_avaliacao_atendimento_id"
                                 referencedColumnNames="id"
                                 referencedTableName="avaliacao_atendimento"/>

        <addForeignKeyConstraint baseColumnNames="clientes_avaliacoes_id"
                                 baseTableName="avaliacao"
                                 constraintName="fk_avaliacao_clientes_avaliacoes_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cliente"/>

    </changeSet>
</databaseChangeLog>
